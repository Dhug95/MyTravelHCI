<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mytravelhci">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Shrine">
        <activity
            android:name=".activity.TripPageActivity"
            android:exported="true"
            android:parentActivityName=".activity.LoggedHomeActivity"></activity>
        <activity
            android:name=".activity.Documentation"
            android:exported="true" />
        <activity android:name=".activity.LoggedHomeActivity" />
        <activity
            android:name=".activity.NotLoggedHomeActivity"
            android:exported="true" />
        <activity
            android:name=".activity.SignupPage"
            android:label="Sign Up"
            android:parentActivityName=".activity.NotLoggedHomeActivity" />
        <activity
            android:name=".activity.Currency"
            android:exported="true" />
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.AddTrip"
            android:exported="true"
            android:parentActivityName=".activity.LoggedHomeActivity" />
        <activity
            android:name=".activity.EditTrip"
            android:exported="true"
            android:parentActivityName=".activity.TripPageActivity" />
    </application>

</manifest>